// You are using GCC
#include <iostream>
using namespace std;
int main(){
    string text , pattern;
    getline(cin , text);
    cin >> pattern;
    int len1 = text.length();
    int len2 = pattern.length();
    bool found = false;
    int i ,j;
    for(i=0 ; i < len1 ; i++){
        for(j = 0 ; j < len2 ; j++){
            if(text[ i + j ] != pattern[j]){
                break;
            }
        }
        if(j == len2){
            cout << "Pattern found at index " << i << " " << endl;
            found = true ;
        }
    }
if(!found){
    cout << "Pattern not found in the text."<<endl;
}
return 0;
}
