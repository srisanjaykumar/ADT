#include<iostream>
using namespace std;
int isprime(int n)
{
    for(int i=2;i<=n/2;i++){
        if(n%i==0){
            return 0;
        }
    }
    return 1;
}
void primecheck(int num){
    for(int i=2;i<=num;i++){
        if(isprime(i))
        {
            cout<<i<<" ";
        }
    }
}
int main(){
    int num;
    cin>>num;
    primecheck(num);
}

-----------------------------------------------------------------------------------------------
N th prime numbers coding

#include <iostream>
using namespace std;

bool isPrime(int n) {
    if (n <= 1) return false;
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0) return false;
    }
    return true;
}

int main() {
    int n, count = 0, num = 2;
    cout << "Enter n: ";
    cin >> n;

    while (true) {
        if (isPrime(num)) {
            count++;
            if (count == n) {
                cout << n << "th prime is: " << num;
                break;
            }
        }
        num++;
    }
    return 0;
}
