//floyds warshall algorithm

#include<iostream>
#include<vector>
using namespace std;
int main (){
    int n ;
    cin >> n ;
    vector<vector<int>>graph(n , vector<int>(n) );
    for(int i = 0 ; i < n ; i++){
        for(int j = 0; j < n ; j++ ){
            cin >> graph[i][j];
        }
    }
    for(int k = 0 ; k < n ; k++){ //intermediate
        for(int i = 0 ; i < n ; i++){ //row
            for(int j = 0 ; j < n ; j++){ //coloum
                if(graph[i][k] + graph[k][j] < graph[i][j]){
                   graph[i][j] = graph[i][k] + graph[k][j];  
                }
            }
        }
    }
    for(int i = 0 ; i < n ; i++){
        for(int j = 0 ; j < n ; j++){
            cout<<graph[i][j]<<" ";
        }
        cout<<endl;
    }
}
