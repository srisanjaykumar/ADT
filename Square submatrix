#include<bits/stdc++.h>
using namespace std;
int main(){
    int R , C , i , j , MAX = 1 ;
    cin >> R >> C;
    vector<vector<int>> matrix(R , vector<int>(C));
    vector<vector<int>> dp(R , vector<int>(C , 0));
    for( i = 0 ; i < R ; i++){
        for( j = 0 ; j < C ; j++){
            cin >> matrix[i][j];
        }
    }
    for(i = 0 ; i < R ; i++){
        dp[i][0] = matrix[i][0];
    }
    for(j = 0 ; j < C ; j++){
        dp[0][j] = matrix[0][j];
    }
    for(i = 1 ; i < R ; i++){
        for(j = 1 ; j < C ; j++){
            if(matrix[i][j] == 1){
                dp[i][j] = 1 + min({ dp[i - 1][j] , dp[i][j - 1] , dp[i - 1][j - 1]});
                MAX = max(MAX , dp[i][j]);
            }
        }
    }
    for(i = 0 ; i < MAX ; i++){
        for(j = 0 ; j < MAX ; j++){
            cout<<1<<" ";
            
        }
        cout<<endl;
    }
    return 0;
}
